        MENUITEM "写入数据",                    ID_DB_WRITE
        MENUITEM "添加表",                      ID_TABLE_ADD
        MENUITEM "添加记录",                    ID_RECORD_ADD
    END
    POPUP "数据库操作(DAO)"
    BEGIN
        MENUITEM "读取数据",                    ID_DAO_READ
        MENUITEM "写入数据",                    ID_DAO_WRITE
        MENUITEM "编辑数据",                    ID_DAO_EDIT
        MENUITEM "创建数据库",                  ID_CREATE_DB
    END
